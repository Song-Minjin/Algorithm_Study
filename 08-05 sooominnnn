class Solution {
    public String solution(String s) {
        
        String answer = "";
        int num = s.length();
        int num2 = num/2;
        //짝수인 경우
        if(num % 2 == 0){
            answer = s.substring(num2-1, num2+1);
       //홀수인 경우     
        }else{
            answer = s.substring(num2, num2+1);
        }
        return answer;
    }
}
